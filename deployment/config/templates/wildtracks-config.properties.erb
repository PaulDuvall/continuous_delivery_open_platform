// Database Configuration
dataSource.url=<%= dataSource_url %>
dataSource.username=<%= dataSource_username %>
dataSource.password=<%= dataSource_password %>

// Temporary Work Folder. /var/tmp should be writeable by tomcat user (preferably any user). 
dataStorage.workDir=<%= dataStorage_workDir %>

// FTP Information.
dataStorage.ftpUrl=<%= dataStorage_ftpUrl %>
dataStorage.ftpUsername=<%= dataStorage_ftpUsername %>
dataStorage.ftpPassword=<%= dataStorage_ftpPassword %>

// Database backup script.
databaseBackup.script.file=<%= databaseBackup_script_file %>
